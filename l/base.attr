/*
 * define attribute pop
 */
LDEFATTR(pop,"pop",{
    vps_cfg* parent;
    vps_cfg* cfg;
    vcfg_graph* grp;
    vps_inst* inst;
    last_obj* obj;
    last_obj* next;

    parent = req->parent;
    
    if(!LATTR_CONTEXT_BLOCK(parent)){
      uabort("pop inst can not use here!");
    }
    cfg = (vps_cfg*)parent;
    if (cfg->parent->t != vcfgk_grp) {
      uabort("push inst must be use at subr!");
    }
    grp = (vcfg_graph*)cfg->parent;
    next = last_cdr(req->ast_obj);
    if (next) {
      if (next->t != lastk_cons) {
	uabort("top inst error!");
      }
      obj = last_car(next);
      if (!obj) {
	uabort("top has no operand!");
      }
      if (obj->t == lastk_symbol) {
	last_symbol* sym = (last_symbol*)obj;
	inst = vps_ipopdt(req->vps,grp,sym->name);
      } else {
	uabort("pop inst error!");
	inst = NULL;
      }
    } else {
      inst = vps_ipopv(req->vps);
    }
    
    ulog("pop");
    LATTR_RETURN(lar_vps_apd,inst);
  })

