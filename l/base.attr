/*
 * common function define subr
 */
static vcfg_graph* subr(last_attr_req* subreq){ 
  vps_cntr* vps;
  vps_mod* top;
  last_obj* next;
  last_obj* obj;
  last_symbol* subr_name;
  last_obj* args;
  vcfg_graph* grp;
  vcfg_block* blk;
  last_attr_req req;

  vps = subreq->vps;
  top = subreq->top;
  req.vps = subreq->vps;
  req.top = subreq->top;
  req.reader = subreq->reader;

  next = last_cdr(subreq->ast_obj);
  if(!next || next->t != lastk_cons){
    uabort("subr error!");
  }

  obj = last_car(next);
  if(!obj || obj->t != lastk_symbol){
    uabort("subr name invalid!");
  }
  subr_name = (last_symbol*)obj;

  next = last_cdr(next);
  if(!next || next->t != lastk_cons){
    uabort("subr error!");
  }

  grp = vcfg_graph_new(vps);
  if(!grp){
    uabort("subr new cfg error!");
  }
  grp->parent = (vps_t*)top;

  obj = last_car(next);
  if(obj){
    int arg_idx = 0;
    if(obj->t != lastk_cons){
      uabort("subr args invalid!");
    }
    args = obj;
    /* args procedure */
    while(args != NULL){
      last_obj* arg = last_car(args);
      last_symbol* sym;
      vps_data* data;

      if (arg->t != lastk_symbol) {
	uabort("arg not a symbol!");
      }
      sym = (last_symbol*)arg;
      data = vps_any_new(vps,sym->name,vstk_stack);
      if(!data){
	uabort("vps_any_new error!");
      }
      data->idx = arg_idx;
      data->scope = VPS_SCOPE_LOCAL;
      vcfg_grp_params_apd(grp,data);
      args = last_cdr(args);
      arg_idx++;
    }
  }

  grp->name = subr_name->name;
  blk = vcfg_block_new(vps);
  if(!blk){
    uabort("subr new blk error!");
  }
  blk->parent = (vps_t*)grp;
  vcfg_grp_cdapd(vps,grp,(vps_cfg*)blk);
  req.parent = (vps_cfg*)blk;
  
  next = last_cdr(next);
  while(next){
    last_obj* car;
    last_symbol* inst_name;
    if(next->t != lastk_cons){
      uabort("subr error!");
    }
    obj = last_car(next);
    if(obj->t != lastk_cons){
      uabort("subr error!");
    }
    car = last_car(obj);
    if(!car || car->t != lastk_symbol){
      uabort("subr inst invalid!");
    }
    inst_name = (last_symbol*)car;
    if(inst_name->attr){
      last_attr* attr;
      last_attr_res res;
      int retval;

      req.ast_obj = obj;
      attr = inst_name->attr;
      if (attr == &last_attr_symcall) {
	uabort("not a inst!");
      }
      retval = (attr->action)(&req,&res);
      if (retval != 0) {
	if (res.res_type == lar_vps_apd) {
	  /* append instruction */
	  vps_t* res_vps = res.res_vps;
	  if (!res_vps) {
	    uabort("res_vps can not be null!");
	  }
	  if(res_vps->t != vpsk_inst){
	    uabort("not a inst!");
	  }
	  vcfg_blk_apd(blk,(vps_inst*)res_vps);
	} else if (res.res_type == lar_blk_new) {
	  /* append a new block */
	  vps_t* res_vps = res.res_vps;
	  if (!res_vps) {
	    uabort("res_vps can not be null!");
	  }
	  if (res_vps->t != vcfgk_blk) {
	    uabort("not a block");
	  }
	  blk = (vcfg_block*)res_vps;
	  blk->parent = (vps_t*)grp;
	  vcfg_grp_cdapd(vps,grp,(vps_cfg*)blk);
	  req.parent = (vps_cfg*)blk;
	} else {
	  /* others */
	  uabort("unknow res_type!");
	}
      }
    } else {
      uabort("symbol not define attribute!");
    }
    next = last_cdr(next);
  }
    
  return grp;
}

/*
 * define attribute subr
 */
LDEFATTR(g_subr,"g-subr",{
    vcfg_graph* grp;

    if (!LATTR_CONTEXT_FILE(req->parent)) {
      uabort("g-subr inst can not use here!");
    }
    grp = subr(req);
    grp->scope = VPS_SCOPE_GLOBAL;
    vps_mod_code_put(req->top,grp);
    LATTR_RETURN_VOID;
  })
  
